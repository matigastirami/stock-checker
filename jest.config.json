{
    "collectCoverage": true,
    "collectCoverageFrom": [
        "**/*.{js,jsx}"
    ],
    "coverageDirectory": "/coverage",
    "coverageProvider": "v8",
    "coverageThreshold": {
        "global": {
            "branches": 90,
            "functions": 90,
            "lines": 90,
            "statements": 90
        }
    },
    "testPathIgnorePatterns": [
        "/node_modules/",
        "/bin/",
        "/helpers/index.js"
    ]
}